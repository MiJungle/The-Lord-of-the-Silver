# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def sortList(self, head: Optional[ListNode]) -> Optional[ListNode]:
        p = head
        tmp = []
        while p:
            tmp.append(p.val)
            p = p.next

        tmp.sort()

        p = head
        for i in range(len(tmp)):
            p.val = tmp[i]
            p = p.next
        return head
